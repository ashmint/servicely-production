<div class="container">
  <div class="column mt-5">
      <div class="row-6">
          <div class="d-flex gap-3">
              <p><strong>Name</strong>: <%= user.firstName %></p>
              <p><strong>Location</strong>: <%= user.address %></p>
          </div>
          <div class="mt-5">
            <h2>Add Vehicle</h2>
            <form action="/user/addVehicle/<%= user.id %>" method="POST">
              <div class="d-flex flex-wrap justify-content-between gap-2">
              <div class="mb-3">
                  <label for="manufacturer" class="form-label">Manufacturer</label>
                  <input type="text" class="form-control" id="manufacturer" name="manufacturer">
              </div>
              <div class="mb-3">
                <label for="model" class="form-label">Model</label>
                <input type="text" class="form-control" id="model" name="model">
              </div>
              <div class="mb-3">
                <label for="makeYear" class="form-label">Make year</label>
                <input type="text" class="form-control" id="makeYear" name="makeYear">
              </div>
              <div class="mb-3">
                <label for="vinNumber" class="form-label">VIN Number</label>
                <input type="text" class="form-control" id="vinNumber" name="vinNumber">
              </div>
              <div class="mb-3">
                <label for="numberPlate" class="form-label">Number Plate</label>
                <input type="text" class="form-control" id="numberPlate" name="numberPlate">
              </div>
              </div>
              <button type="submit" class="btn btn-primary" value="Upload">Submit</button>
            </form>
          </div>
      </div>

      
      <div class="mt-3">
      <h2>Vehicles</h2>
      <ul class="list-group list-group-light">
        <% for(var i=0; i<vehicles.length; i++) {%>
          <a href = "/vehicle/<%= vehicles[i]._id%>">
          <li class="shadow list-group-item d-flex justify-content-between align-items-center">
          <div>
            <div class="fw-bold"><%= `${vehicles[i].manufacturer} ${vehicles[i].model} ${vehicles[i].makeYear}`%></div>
            <div class="text-muted"><%= `VIN: ${vehicles[i].vinNumber}` %></div>
            <div class="text-muted"><%= `Number Plate: ${vehicles[i].numberPlate}` %></div>
          </div>
          <span class="badge rounded-pill text-bg-warning">In-queue</span>
          </li>
          </a>
        <% } %>
